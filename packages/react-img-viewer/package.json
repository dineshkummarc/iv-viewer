    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "husky": "^9.0.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "size-limit": "^11.1.4",
    "storybook": "^8.1.10",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
